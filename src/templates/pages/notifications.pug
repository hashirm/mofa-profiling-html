                            #leader-pane.tab-pane.notification-pane.fade(role='tabpanel' aria-labelledby='leader-tab' tabindex='0')
                                .notification-items-wrapper
                                nav#pagination-leader-notifications.pagination-all-notifications.mt-3.d-flex.justify-content-center(aria-label='Pagination for Leader Notifications')
                        // Error Modal
                        #errorModal.modal.fade(tabindex='-1' aria-labelledby='errorModalLabel' aria-hidden='true')
                            .modal-dialog.modal-dialog-centered
                                .modal-content
                                    .modal-header
                                        h5#errorModalLabel.modal-title.fs-5 @Resource.Error
                                        button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
                                    .modal-body
                                        span#errorMessage Something went wrong!
                                    .modal-footer
                                        button.btn.bg-transparent.text-black(data-bs-dismiss='modal' type='button') @Resource.Close


