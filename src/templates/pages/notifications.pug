                .mofa-cms-content.designation.parameter.resources.notifications
                    nav(aria-label="breadcrumb") 
                        ol.breadcrumb.align-items-center 
                            li.breadcrumb-item(aria-current="page")
                                img(src="assets/images/dashboard-icon.png", alt="", srcset="")
                                | Dashboard
                            li.breadcrumb-item.active(aria-current="page")
                                | Notifications
                    .recent-searches
                        .notifications-header
                            .header
                                h4 Notifications
                            .read-action
                                a#mark-all-as-read-Btn(href='javascript:void(0)') Mark All As Read
                        ul#profiling-tabs.nav.nav-pills.notification-nav-pills.w-100.flex-nowrap(role='tablist')
                            li.nav-item.w-50(role='presentation')
                                button#all-tab.w-100.nav-link.active(data-type='all' data-bs-toggle='tab' data-bs-target='#all-pane' type='button' role='tab' aria-controls='all-pane' aria-selected='true')
                                    | All
                                    span#notification-number-all.notification-number 
                                        | 9+
                            li.nav-item.w-100(role='presentation')
                                button#country-tab.w-100.nav-link(data-type='country' data-bs-toggle='tab' data-bs-target='#country-pane' type='button' role='tab' aria-controls='country-pane' aria-selected='false')
                                    | Country Profiling
                                    span#notification-number-country.notification-number.d-none
                            li.nav-item.w-100(role='presentation')
                                button#state-tab.w-100.nav-link(data-type='state' data-bs-toggle='tab' data-bs-target='#state-pane' type='button' role='tab' aria-controls='state-pane' aria-selected='false')
                                    | State Profiling
                                    span#notification-number-state.notification-number.d-none
                            li.nav-item.w-100(role='presentation')
                                button#leader-tab.w-100.nav-link(data-type='leader' data-bs-toggle='tab' data-bs-target='#leader-pane' type='button' role='tab' aria-controls='leader-pane' aria-selected='false')
                                    | Leader Profiling
                                    span#notification-number-leader.notification-number.d-none
                        #profiling-tabs-content.tab-content.mt-3
                            // All
                            #all-pane.tab-pane.notification-pane.fade.show.active(role='tabpanel' aria-labelledby='all-tab' tabindex='0')
                                .notification-items-wrapper
                                    .notification-item.d-flex.justify-content-between 
                                            .notification-content.d-flex.align-items-start.gap-2 
                                                .d-flex.align-items-center.gap-1
